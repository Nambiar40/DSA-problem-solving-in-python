def nonDivisibleSubset(k, s):
    # Frequency array for remainders
    freq = [0] * k
    for num in s:
        freq[num % k] += 1

    # At most one number with remainder 0
    result = min(freq[0], 1)

    # Handle pairs of remainders i and k-i
    for i in range(1, (k // 2) + 1):
        if i != k - i:
            result += max(freq[i], freq[k - i])
        else:
            # i == k-i means k is even & remainder = k/2, take only 1
            result += 1 if freq[i] > 0 else 0

    return result
